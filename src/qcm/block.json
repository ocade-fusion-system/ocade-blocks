{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ocade-blocks/qcm",
	"version": "1.0.0",
	"category": "widgets",
  "keywords": ["ocade", "qcm", "questions", "test"],
	"textdomain": "ocade-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  "supports": {
    "anchor": true,
    "align": ["wide", "full"],
    "defaultAlign": "wide"
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "preview": true
    },
    "question": {
      "type": "string",
      "default": ""
    },
    "options": {
      "type": "array",
      "default": ["", ""]
    }
  }
}